{
    "store.custom#footerBar": {
        "blocks": [
            "sticky-layout"
        ]
    },

    "sticky-layout":{
        "props": {
            "position": "bottom"
          },
        "children": [
            "flex-layout.row"
        ]        
    },

    "flex-layout.row": {
        "children": [
            "flex-layout.col#1",
            "flex-layout.col#2",
            "flex-layout.col#3",
            "flex-layout.col#4"
        ]    
    },

    "flex-layout.col#1": {
        "children":[
            "image#sticky1"
        ]
    },

    "flex-layout.col#2": {
        "children":[
            "image#sticky2"
        ]
    },

    "flex-layout.col#3": {
        "children":[
            "image#sticky3"
        ]
    },

    "flex-layout.col#4": {
        "children":[
            "image#sticky4"
        ]
    },

    "image#sticky1":{
        "props":{
            "src": "assets/img/user.png",
            "maxWidth": 80
        }
    },

    "image#sticky2":{
        "props":{
            "src": "",
            "maxWidth": 80
        }
    },

    "image#sticky3":{
        "props":{
            "src": "",
            "maxWidth": 80
        }
    },

    "image#sticky4":{
        "props":{
            "src": "",
            "maxWidth": 80
        }
    }
}
